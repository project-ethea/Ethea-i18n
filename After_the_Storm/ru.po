#. [side]: id=Inara, type=Quenoth Ranger
#: After_the_Storm/episode1/scenarios/06_02_Elves_of_a_Different_Land.cfg:156
msgid "Inara"
msgstr "Инара"

#. [unit]: id=Ainael, type=Quenoth Warrior
#: After_the_Storm/episode1/scenarios/06_02_Elves_of_a_Different_Land.cfg:168
msgid "Ainael"
msgstr "Айнэль"

#. [unit]: id=Remir-Taen, type=Tauroch Protector
#: After_the_Storm/episode1/scenarios/06_02_Elves_of_a_Different_Land.cfg:181
msgid "Remir-Taen"
msgstr "Ремир-Таэн"

